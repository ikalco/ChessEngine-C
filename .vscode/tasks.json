{
	"tasks": [
		{
			"type": "shell",
			"label": "Build Project with Make",
			"command": "/usr/bin/make",
			"args": [
				"all",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
		}
	],
	"version": "2.0.0"
}
